<project name="Calibre">
    <skin>
        <groupId>com.bernardomg.maven.skins</groupId>
        <artifactId>docs-maven-skin</artifactId>
    </skin>

    <custom>
        <skinConfig>
            <topNav>
                <menu>Reports</menu>
            </topNav>
            <iconNav>
                <menu>Icons</menu>
            </iconNav>
        </skinConfig>
    </custom>

    <body>
        <menu name="Reports" inherit="bottom">
            <item name="Summary" href="./summary.html" />
            <item name="Dependencies" href="./dependencies.html" />
            <item name="Dependency Info" href="./dependency-info.html" />
            <item name="Plugin Management" href="./plugin-management.html" />
            <item name="SCM" href="./scm.html" />
        </menu>
        <menu name="Icons" inherit="bottom">
            <item name="JavaDocs" img="book" href="./apidocs/index.html" />
            <item name="GitLab" img="gitlab" href="${project.scm.url}" />
            <item name="Download" img="download" href="${project.scm.url}/-/jobs/artifacts/master/raw/target/${project.name}.jar?job=build" />
        </menu>
    </body>
</project>
